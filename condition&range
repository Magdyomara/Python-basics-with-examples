if and else
m = 5
t = 10
if m > t 
    print('true')
else:
    print('wrong')
  = wrong
 _____________________________________________________________________________________________________________ 
  if and  elif,else   
m = 5
t = 10
if m > t 
    print('true A')
elif m < t 
    print('true B')  
else:
    print('wrong')
  = true B
 _____________________________________________________________________________________________________________ 
and & or
a = 100
b = 200
if a == 100 and b == 200 :
    print('true')
else:
    print('wrong')
= true
 _____________________________________________________________________________________________________________ 
a = 100
b = 200
if a == 100 or b == 300 :
    print('true')
else:
    print('wrong')
= true
